{
  "name": "rehi",
  "displayName": "Rehi",
  "version": "0.0.1",
  "description": "Saving article and making highlight",
  "author": "tuyetlangsa",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "math-intrinsics": "1.1.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "crypto-browserify": "^3.12.1",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "manifest_version": 3,
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "activeTab",
      "commands",
      "scripting"
    ],
    "commands": {
      "get-raw-html": {
        "suggested_key": {
          "default": "Alt+R"
        },
        "description": "Get raw HTML of current page"
      }
    }
  }
}
